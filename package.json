  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-table": "^6.8.5",
    "bootstrap": "^5.3.8",
    "dotenv-cli": "^10.0.0",
    "env-cmd": "^11.0.0",
    "handlebars": "^4.7.7",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-ga": "^3.3.1",
    "react-lottie": "^1.2.10",
    "react-pdf": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "react-router-hash-link": "^1.2.2",
    "react-table": "^6.10.0",
    "react-visibility-sensor": "^5.1.1",
    "serialize-javascript": "^6.0.2",
    "styled-components": "^6.1.19"
  },
  "scripts": {
    "watch": "react-scripts start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:staging": "dotenv -e .env.staging npm run build",
    "build:production": "dotenv -e .env.production npm run build",
    "test-ci": "cross-env CI=true react-scripts test --watchAll=false --passWithNoTests"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/promise-polyfill": "^6.0.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-lottie": "^1.2.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "cross-env": "^10.0.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-watch-typeahead": "^3.0.1",
    "react-scripts": "^5.0.1",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": "22.x"
  },
  "resolutions": {
    "@types/react": "*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(undici|@emailjs|@babel|@testing-library)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ]
  }
}
